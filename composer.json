{
    "name": "nickfan/composer-mns",
    "description": "composer version of Aliyun Message Service(MNS),Base On Laravel-MNS",
    "keywords": ["aliyun", "php", "composer", "mns"],
    "license": "MIT",
    "authors": [
        {
            "name": "Abraham Greyson",
            "email": "82011220@qq.com"
        },
        {
            "name": "Nick Fan",
            "email": "nickfan81@gmail.com"
        }
    ],
    "support": {
        "issues": "https://github.com/nickfan/composer-mns/issues",
        "source": "https://github.com/nickfan/composer-mns"
    },
    "require": {
        "guzzlehttp/guzzle": "~6.0",
        "guzzlehttp/psr7": "~1.3"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.1",
        "mockery/mockery": "~0.9.4"
    },
    "autoload": {
        "psr-4": {
            "AliyunMNS\\": "lib/AliyunMNS/",
            "ComposerMns\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ComposerMns\\Test\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "extra": {
        "branch-alias": {
            "dev-master": "5.2-dev"
        }
    }
}
